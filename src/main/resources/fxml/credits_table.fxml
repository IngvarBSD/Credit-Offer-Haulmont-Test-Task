<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxWidth="Infinity" HBox.hgrow="ALWAYS" fx:controller="glebi.javafx.app.CreditsController" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TableView fx:id="creditsTable" VBox.vgrow="ALWAYS" />
        <HBox prefHeight="22.0" spacing="30.0">
            <children>
                <Button onAction="#btnInsertAction" alignment="BOTTOM_CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="25.0" prefWidth="130.0" text="Добавить запись" HBox.hgrow="ALWAYS" />
                <Button onAction="#btnDeleteAction" alignment="BOTTOM_CENTER" layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="25.0" prefWidth="130.0" text="Удалить запись" HBox.hgrow="ALWAYS" />
                <Button onAction="#btnUpdateAction" alignment="BOTTOM_CENTER" layoutX="140.0" layoutY="10.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="25.0" prefWidth="130.0" text="Редактировать запись" HBox.hgrow="ALWAYS" />
            </children>
            <padding>
                <Insets bottom="2.0" left="5.0" right="5.0" top="2.0" />
            </padding>
        </HBox>
    </children>
</VBox>